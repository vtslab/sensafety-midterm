#include <stdio.h>
#include <stdlib.h>
#include <string.h>

/** Type definitions **/
typedef unsigned char BOOLEAN;
typedef unsigned char INT8U;
typedef signed char INT8S;
typedef unsigned int INT16U;
typedef signed int INT16S;
typedef unsigned long INT32U;
typedef signed long INT32S;
typedef float FP32;
typedef double FP64;

#define MQTT_ILP_ID "1"

int main(void) {

	INT8U message[4];
	INT8U driver = 0;
	INT8U breakChar = '/';
	INT8U value = 255;


	strcpy( (char *) message, (char * ) driver);
	strcat( (char *) message, (char * ) breakChar);
	strcat( (char *) message, (char * ) value);

	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	return EXIT_SUCCESS;
}

