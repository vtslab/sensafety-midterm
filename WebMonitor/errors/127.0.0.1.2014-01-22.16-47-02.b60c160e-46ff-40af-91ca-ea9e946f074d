S'# -*- coding: utf-8 -*-\n\nimport os, time, datetime, email\n\nMAX_STILLS_PER_MAC = 6\n\ndef eventbackforth():  # Accept backforth events\n    pv = request.post_vars\n    mactoday = db((db.toplist.mac == pv.mac) &\n                  (db.toplist.ddate == pv.ddate)).select().first()\n    if mactoday:\n        print \'update with: \', str(pv)\n        mactoday.update_record(backforth = mactoday.backforth + 1, endtime = pv.ttime)\n    else:\n        print \'insert: \', str(pv)\n        db.toplist.insert(**{\n            \'mac\': pv.mac,\n            \'ddate\': pv.ddate,\n            \'begintime\': pv.ttime,\n            \'endtime\': pv.ttime,\n            \'backforth\': 1})\n    return str(pv)  # Echo posted parameters\n\ndef eventstill():  # Accept video still events\n    print \'eventstill\'\n    pv = request.post_vars\n    msg = email.message_from_string(pv.email)\n    parts = [part.get_payload(decode = True) for part in msg.walk()]\n    print \'len(parts)\', len(parts)\n    if not len(parts) == 3:\n        return \'Error. Invalid email message\'\n    path = \'/home/m-doit/Python/web2py/applications/DB19/static/images/\'\n    filename += pv.ddate + \'_\' + pv.ttime + \'.jpg\'\n    f = open(path + filename,\'wb\')\n    f.write(parts[2])\n    f.close()\n    print \'insert: \', str(pv)\n    db.foto.insert(**{\n        \'mac\': pv.mac,\n        \'ddate\': pv.ddate,\n        \'ttime\': pv.ttime,\n        \'url\': URL(\'static\', \'images/\' + filename)\n        })\n    if db(db.foto.mac == pv.mac).count() > MAX_STILLS_PER_MAC:\n        first = db(db.foto.mac == pv.mac).select().first()\n        del db.foto[first.id]\n        os.remove(path + first.filename)\n    return str(pv)[:160] # Echo posted parameters\n\ndef toplist():\n    return dict()  # No input for view\n\ndef vandaag():\n    # https://datatables.net/release-datatables/examples/ajax/ajax.html\n    tvandaag = []\n    t = time.localtime()\n    today = \'%i %02i %02i\' %(t[0], t[1], t[2])\n    for row in db(db.toplist.ddate == today).select(db.toplist.ALL):\n        if db(db.foto.mac == row.mac).count():\n            link = \'<a href="\' + URL(\'event\',\'foto.html/\' + row.mac) + \'">\' \\\n                   + row.mac + \'</a>\'\n        else:\n            link = row.mac\n        tvandaag.append([link, str(row.backforth), \n                        row.begintime[0:8], row.endtime[0:8]])\n    return {\'aaData\': tvandaag}\n\ndef hoogst():\n    # https://datatables.net/release-datatables/examples/ajax/ajax.html\n    thoogst = []\n    for row in db().select(db.toplist.ALL, orderby = ~db.toplist.backforth, \n                           limitby =( 0, 20)):\n        period = str(datetime.timedelta(seconds =\n                 time.mktime(time.strptime(row.endtime[0:8] + \' 1970\', \n                 "%H:%M:%S %Y")) -  # mktime only works since 1970\n                 time.mktime(time.strptime(row.begintime[0:8] + \' 1970\', \n                 "%H:%M:%S %Y"))\n                 ))\n        if db(db.foto.mac == row.mac).count():\n            link = \'<a href="\' + URL(\'event\',\'foto.html/\' + row.mac) + \'">\' \\\n                   + row.mac + \'</a>\'\n        else:\n            link = row.mac\n        thoogst.append([link, str(row.backforth), row.ddate, period])\n    return {\'aaData\': thoogst}\n    \ndef foto():\n    if len(request.args) == 0:\n        mac = \'unknown\'\n    else:\n        mac = request.args[0]\n    fotolist = []\n    for row in db(db.foto.mac == mac).select(db.foto.filename):\n        fotolist.append(IMG(_src = row.url))\n    return dict(mac = mac, fotolist = fotolist)\n\n\nresponse._vars=response._caller(eventstill)\n'
